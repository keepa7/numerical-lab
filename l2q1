#include <stdio.h>

int main() {
	 printf("\n Program by Keepa");
    int n, i, j;
    float x[20], y[20], xp, yp = 0, term;

    printf("\n Enter number of data points: ");
    scanf("%d", &n);

    printf("Enter the data points (x y):\n");
    for(i = 0; i < n; i++) {
        scanf("%f %f", &x[i], &y[i]);
    }

    printf("Enter the value of x to find f(x): ");
    scanf("%f", &xp);

    for(i = 0; i < n; i++) {
        term = y[i];
        for(j = 0; j < n; j++) {
            if(j != i) {
                term = term * (xp - x[j]) / (x[i] - x[j]);
            }
        }
        yp = yp + term;
    }

    printf("The approximate value of f(%.2f) is %.4f\n", xp, yp);
   

    return 0;
}
