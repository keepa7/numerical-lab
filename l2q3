#include <stdio.h>

int main() {
	 printf("\n Program by Keepa");
    int n, i, j;
    float x[20], y[20][20], xp, h, p, sum, term;

   
    printf(" \n Enter number of data points: ");
    scanf("%d", &n);

    printf("Enter the values of x and y:\n");
    for (i = 0; i < n; i++) {
        scanf("%f %f", &x[i], &y[i][0]);
    }

    
    for (j = 1; j < n; j++) {
        for (i = n - 1; i >= j; i--) {
            y[i][j] = y[i][j - 1] - y[i - 1][j - 1];
        }
    }

    
    printf("Enter the value of x to interpolate: ");
    scanf("%f", &xp);

    h = x[1] - x[0];
    p = (xp - x[n - 1]) / h;

    sum = y[n - 1][0];
    term = 1;

    
    for (i = 1; i < n; i++) {
        term = term * (p + (i - 1)) / i;
        sum = sum + term * y[n - 1][i];
    }

    printf("Interpolated value at x = %.3f is %.6f\n", xp, sum);

    return 0;
}
